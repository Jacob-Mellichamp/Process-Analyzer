<Window x:Class="Process_Analyzer_gui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Process_Analyzer_gui"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="Submit" Content="Attach" HorizontalAlignment="Center" Margin="0,319,0,0" VerticalAlignment="Top" Width="120" Height="38" Click="GetProcessInfoButton_Click"/>
        <Label Content="Process Analyzer v1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="ProcessComboBox" HorizontalAlignment="Center" Margin="0,126,0,0" VerticalAlignment="Top" Width="280"/>
        <Label Content="Choose A Process To Attach too" HorizontalAlignment="Center" Margin="0,53,0,0" VerticalAlignment="Top" FontSize="30"/>

    </Grid>
</Window>
